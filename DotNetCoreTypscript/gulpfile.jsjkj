/// binding AfterBuild='default' Clean='clean' />

var gulp = require("gulp");
var ts = require("gulp-typescript");

gulp.task("default", function (done) {
    return gulp.src('Scripts/**/*.ts')
        .pipe(ts({
            noImplicitAny: true,
            outFile: 'output.js'
        }))
        .pipe(gulp.dest('built/local'));
});